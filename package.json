{
	"name": "vuetify-birthdaypicker",
	"version": "0.0.3",
	"description": "Birthday picker component for Vuetify",
	"type": "module",
	"license": "MIT",
	"homepage": "https://github.com/igorexa34314/vuetify-birthdaypicker#readme",
	"bugs": {
		"url": "https://github.com/igorexa34314/vuetify-birthdaypicker/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/igorexa34314/vuetify-birthdaypicker.git"
	},
	"author": "Igor Kononenko <igorkono343@gmail.com>",
	"keywords": [
		"birthdaypicker",
		"vuetify-birthdaypicker",
		"vuetify-components",
		"vuetify",
		"vue3",
		"vue",
		"vuejs"
	],
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"dev:playground": "vite",
		"build:playground": "vite build",
		"preview:playground": "vite preview",
		"test": "vitest",
		"typecheck": "vue-tsc --noEmit",
		"release": "bumpp && pnpm publish",
		"prepublishOnly": "pnpm run build",
		"lint": "eslint src/",
		"format": "prettier --write --cache src/"
	},
	"peerDependencies": {
		"vue": "^3.0.0",
		"vuetify": "^3.0.0"
	},
	"devDependencies": {
		"@types/node": "^24.9.1",
		"@vitejs/plugin-vue": "^6.0.1",
		"@vue/eslint-config-prettier": "^10.2.0",
		"@vue/eslint-config-typescript": "^14.6.0",
		"@vue/test-utils": "^2.4.6",
		"bumpp": "^10.3.1",
		"eslint-plugin-vue": "^10.5.1",
		"happy-dom": "^20.0.8",
		"prettier": "^3.6.2",
		"tsdown": "^0.15.9",
		"typescript": "^5.9.3",
		"vite": "npm:rolldown-vite@^7.1.19",
		"vitest": "^4.0.1",
		"vue": "^3.5.22",
		"vue-tsc": "^3.1.1",
		"vuetify": "^3.10.8"
	},
	"packageManager": "pnpm@10.20.0"
}
